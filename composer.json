{
    "name": "filisko/php-fastcgi-client",
    "type": "library",
    "description": "A modern and lightweight FastCGI client with PSR-7 responses",
    "license": "MIT",
    "keywords": [
        "fastcgi",
        "phpfpm",
        "client",
        "psr7",
        "psr17",
        "testcase",
        "endtoend",
        "functional",
        "integration",
        "unit",
        "tests",
        "sockets"
    ],
    "homepage": "https://github.com/filisko/php-fastcgi-client",
    "support": {
        "issues": "https://github.com/filisko/php-fastcgi-client/issues"
    },
    "require": {
        "php": ">=7.2",
        "filisko/testable-phpfunctions": "dev-main",
        "guzzlehttp/promises": "^2.2",
        "middlewares/utils": "^2 || ^3 || ^4",
        "psr/http-factory": "^1.0",
        "psr/http-factory-implementation": "*",
        "psr/http-message": "^1.0 || ^2.0",
        "psr/http-message-implementation": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7 || ^8 || ^9",
        "friendsofphp/php-cs-fixer": "^3",
        "phpstan/phpstan": "^1 || ^2",
        "guzzlehttp/psr7": "^2.7",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Filisko\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Filisko\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs": "phpcs",
        "cs-fix": "php-cs-fixer fix",
        "phpstan": "phpstan analyse",
        "test": "phpunit",
        "coverage": "phpunit --coverage-html=build/html"
    }
}
